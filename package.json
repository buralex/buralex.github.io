{
  "name": "alex-burlachenko-personal-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && PORT=3005 next dev",
    "build": "npm run lint && rm -rf out && next build",
    "preview": "npx serve@latest out",
    "deploy": "gh-pages -d out --nojekyll --cname buralex.github.io",
    "lint:ts": "tsc --noEmit --pretty",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:stylelint": "stylelint 'src/**/*.{css,scss}'",
    "lint": "npm run lint:ts && npm run lint:eslint && npm run lint:stylelint"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "clsx": "^2.1.1",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "19.1.0",
    "react-slick": "^0.31.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-slick": "^0.23.13",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "gh-pages": "^6.3.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.23.1",
    "stylelint-config-css-modules": "^4.5.1",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "^5"
  }
